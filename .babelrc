{
    "presets": [
        ["@babel/preset-env", { "modules": false, "useBuiltIns": "entry", "corejs": 2 }],
        ["@babel/preset-typescript"],
        ["@babel/preset-react"]
    ],
    "env": {
        "test": {
            "plugins": ["transform-es2015-modules-commonjs"]
        }
    },
    "plugins": [
        ["@babel/plugin-syntax-dynamic-import"]
    ]
}